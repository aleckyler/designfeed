<% if @profile.user_id != current_user.id %>
  <h2>This is not your profile to edit.</h2>
<% else %>

<p id="notice"><%= notice %></p>

<h1>Editing Profile</h1>

<%= render 'form' %>

<%= link_to 'Show', @profile, class: "profile_li" %> |
<%= link_to 'Back', :back, class: "profile_li" %>

<% end %>
